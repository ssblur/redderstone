{
  "multipart": [
    {
      "apply": {
        "model": "redderstone:block/redderstone_wire"
      }
    },
    {
      "when": {
        "wire_east": "true"
      },
      "apply": {
        "model": "redderstone:block/redderstone_wire_east"
      }
    },
    {
      "when": {
        "wire_west": "true"
      },
      "apply": {
        "model": "redderstone:block/redderstone_wire_west"
      }
    },
    {
      "when": {
        "wire_north": "true"
      },
      "apply": {
        "model": "redderstone:block/redderstone_wire_north"
      }
    },
    {
      "when": {
        "wire_south": "true"
      },
      "apply": {
        "model": "redderstone:block/redderstone_wire_south"
      }
    },
    {
      "when": {
        "active": "true",
        "facing": "north"
      },
      "apply": {
        "model": "redderstone:block/furnace_heater_active",
        "y": 90
      }
    },
    {
      "when": {
        "active": "false",
        "facing": "north"
      },
      "apply": {
        "model": "redderstone:block/furnace_heater_inactive",
        "y": 90
      }
    },
    {
      "when": {
        "active": "true",
        "facing": "south"
      },
      "apply": {
        "model": "redderstone:block/furnace_heater_active",
        "y": 270
      }
    },
    {
      "when": {
        "active": "false",
        "facing": "south"
      },
      "apply": {
        "model": "redderstone:block/furnace_heater_active",
        "y": 270
      }
    },
    {
      "when": {
        "active": "true",
        "facing": "east"
      },
      "apply": {
        "model": "redderstone:block/furnace_heater_active",
        "y": 180
      }
    },
    {
      "when": {
        "active": "false",
        "facing": "east"
      },
      "apply": {
        "model": "redderstone:block/furnace_heater_inactive",
        "y": 180
      }
    },
    {
      "when": {
        "active": "true",
        "facing": "west"
      },
      "apply": {
        "model": "redderstone:block/furnace_heater_active",
        "y": 0
      }
    },
    {
      "when": {
        "active": "false",
        "facing": "west"
      },
      "apply": {
        "model": "redderstone:block/furnace_heater_inactive",
        "y": 0
      }
    },
    {
      "when": {
        "active": "false",
        "facing": "up"
      },
      "apply": {
        "model": "redderstone:block/furnace_heater_active_up"
      }
    },
    {
      "when": {
        "active": "true",
        "facing": "up"
      },
      "apply": {
        "model": "redderstone:block/furnace_heater_inactive_up"
      }
    },
    {
      "when": {
        "active": "false",
        "facing": "down"
      },
      "apply": {
        "model": "redderstone:block/furnace_heater_active_down"
      }
    },
    {
      "when": {
        "active": "true",
        "facing": "down"
      },
      "apply": {
        "model": "redderstone:block/furnace_heater_inactive_down"
      }
    }
  ]
}