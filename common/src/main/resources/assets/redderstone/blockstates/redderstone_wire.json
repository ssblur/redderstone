{
  "multipart": [
    {
      "apply": { "model": "redderstone:block/redderstone_wire" }
    },
    {
      "when": { "east": "true" },
      "apply": { "model": "redderstone:block/redderstone_wire_east" }
    },
    {
      "when": { "west": "true" },
      "apply": { "model": "redderstone:block/redderstone_wire_west" }
    },
    {
      "when": { "north": "true" },
      "apply": { "model": "redderstone:block/redderstone_wire_north" }
    },
    {
      "when": { "south": "true" },
      "apply": { "model": "redderstone:block/redderstone_wire_south" }
    },
    {
      "when": { "east_up": "true" },
      "apply": {
        "model": "redderstone:block/redderstone_wire_climb",
        "x": 90,
        "y": 270
      }
    },
    {
      "when": { "south_up": "true" },
      "apply": {
        "model": "redderstone:block/redderstone_wire_climb",
        "x": 90,
        "y": 0
      }
    },
    {
      "when": { "west_up": "true" },
      "apply": {
        "model": "redderstone:block/redderstone_wire_climb",
        "x": 90,
        "y": 90
      }
    },
    {
      "when": { "north_up": "true" },
      "apply": {
        "model": "redderstone:block/redderstone_wire_climb",
        "x": 90,
        "y": 180
      }
    }
  ]
}